{
  "manifest_version": 3,
  "name": "Kuhu",
  "version": "1.0.0",
  "description": "Saving pages made easy with Kuhu!",
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_title": "Kuhu! Bookmarks made easy!",
    "default_popup": "index.html"
  }
}